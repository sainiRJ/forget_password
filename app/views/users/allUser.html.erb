  <table class= "table table-bordered">
    <thead>
       <tr>
           <th>Username</th>
           <th>Email</th>
           <th>First_name</th>
           <th>Last_name</th>
       </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%=user.username %></td>
            <td><%=user.email %></td>
            <td><%=user.first_name %></td>
            <td><%=user.last_name %></td>
        </tr>
        <% end %>
    </tbody>
   </table>
   
   